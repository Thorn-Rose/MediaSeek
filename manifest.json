{  
  "description": "Share video at specific times!",
  "manifest_version": 2,
  "name": "MediaSeek",
  "version": "1.0",

  "background": {
    "scripts": [
      "MediaSeek.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["MediaSeekContentScript.js"]
    }
  ],
  "permissions": ["contextMenus", "activeTab"]
}